    <local:ImageFromPCL x:Key="Circle">Quoridge.Images.Themes.Sky.skyCircle.png</local:ImageFromPCL>
    <local:ImageFromPCL x:Key="FlyCircle">Quoridge.Images.Themes.Sky.skyCircle.png</local:ImageFromPCL>
    
</ResourceDictionary>