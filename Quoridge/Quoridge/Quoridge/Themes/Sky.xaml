                    x:Class="Quoridge.Sky">

    <Color x:Key="NavigationBarColor">#CEE7FF</Color>
    <Color x:Key="NavigationTextColor">#3283B7</Color>
    <Color x:Key="Fab">#A1DAED</Color>
